<?xml version="1.0" encoding="UTF-8"?>
<project name="module_pdf-teacher" default="compile.module.pdf-teacher">
  <dirname property="module.pdf-teacher.basedir" file="${ant.file.module_pdf-teacher}"/>
  
  <property name="module.jdk.home.pdf-teacher" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.pdf-teacher" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.pdf-teacher" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.pdf-teacher" value="-encoding UTF-8 -source 12 -target 12 ${compiler.args}"/>
  
  <property name="pdf-teacher.output.dir" value="undefined"/>
  <property name="pdf-teacher.testoutput.dir" value="undefined"/>
  
  <path id="pdf-teacher.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="pdf-teacher.module.production.classpath">
    <path refid="${module.jdk.classpath.pdf-teacher}"/>
  </path>
  
  <path id="pdf-teacher.runtime.production.module.classpath"/>
  
  <path id="pdf-teacher.module.classpath">
    <path refid="${module.jdk.classpath.pdf-teacher}"/>
  </path>
  
  <path id="pdf-teacher.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.pdf-teacher">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.pdf-teacher">
    <patternset refid="excluded.from.module.pdf-teacher"/>
  </patternset>
  
  
  <target name="compile.module.pdf-teacher" depends="compile.module.pdf-teacher.production,compile.module.pdf-teacher.tests" description="Compile module pdf-teacher"/>
  
  <target name="compile.module.pdf-teacher.production" depends="register.custom.compilers" description="Compile module pdf-teacher; production classes"/>
  
  <target name="compile.module.pdf-teacher.tests" depends="register.custom.compilers,compile.module.pdf-teacher.production" description="compile module pdf-teacher; test classes" unless="skip.tests"/>
  
  <target name="clean.module.pdf-teacher" description="cleanup module">
    <delete dir="${pdf-teacher.output.dir}"/>
    <delete dir="${pdf-teacher.testoutput.dir}"/>
  </target>
</project>