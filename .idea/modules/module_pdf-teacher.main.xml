  </path>
  
  
  <patternset id="excluded.from.module.pdf-teacher.main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.pdf-teacher.main">
    <patternset refid="excluded.from.module.pdf-teacher.main"/>
  </patternset>
  
  <path id="pdf-teacher.main.module.sourcepath">
    <dirset dir="${module.pdf-teacher.main.basedir}/../../src/main">
      <include name="java"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.pdf-teacher.main" depends="compile.module.pdf-teacher.main.production,compile.module.pdf-teacher.main.tests" description="Compile module pdf-teacher.main"/>
  
  <target name="compile.module.pdf-teacher.main.production" depends="register.custom.compilers" description="Compile module pdf-teacher.main; production classes">
    <mkdir dir="${pdf-teacher.main.output.dir}"/>
    <javac2 destdir="${pdf-teacher.main.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.pdf-teacher.main}/javac">
      <compilerarg line="${compiler.args.pdf-teacher.main}"/>
      <bootclasspath refid="pdf-teacher.main.module.bootclasspath"/>
      <classpath refid="pdf-teacher.main.module.production.classpath"/>
      <src refid="pdf-teacher.main.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.pdf-teacher.main"/>
    </javac2>
    
    <copy todir="${pdf-teacher.main.output.dir}">
      <fileset dir="${module.pdf-teacher.main.basedir}/../../src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.pdf-teacher.main.basedir}/../../src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.pdf-teacher.main.tests" depends="register.custom.compilers,compile.module.pdf-teacher.main.production" description="compile module pdf-teacher.main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.pdf-teacher.main" description="cleanup module">
    <delete dir="${pdf-teacher.main.output.dir}"/>
    <delete dir="${pdf-teacher.main.testoutput.dir}"/>
  </target>
