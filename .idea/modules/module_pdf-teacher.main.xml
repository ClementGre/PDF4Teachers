    <path refid="library.gradle:_org.codehaus.mojo:animal-sniffer-annotations:1.17.classpath"/>
  </path>
  
  <path id="pdf-teacher.main.runtime.module.classpath">
    <pathelement location="${pdf-teacher.main.output.dir}"/>
    <path refid="library.gradle:_org.apache.pdfbox:pdfbox:2.0.13.classpath"/>
