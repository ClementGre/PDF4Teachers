  <property name="module.jdk.classpath.pdf-teacher.main" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.pdf-teacher.main" value="-encoding UTF-8 -source 8 -target 1.8 ${compiler.args}"/>
  
  <property name="pdf-teacher.main.output.dir" value="${module.pdf-teacher.main.basedir}/../../build/classes/java/main"/>
  <property name="pdf-teacher.main.testoutput.dir" value="${module.pdf-teacher.main.basedir}/../../build/classes/java/main"/>
  
  <path id="pdf-teacher.main.module.bootclasspath">
