    <path refid="library.gradle:_org.checkerframework:checker-qual:2.8.1.classpath"/>
    <path refid="library.gradle:_com.google.errorprone:error_prone_annotations:2.3.2.classpath"/>
    <path refid="library.gradle:_com.google.j2objc:j2objc-annotations:1.3.classpath"/>
    <path refid="library.gradle:_org.codehaus.mojo:animal-sniffer-annotations:1.17.classpath"/>
  </path>
  
  <path id="pdf-teacher.main.module.classpath">
    <path refid="${module.jdk.classpath.pdf-teacher.main}"/>
    <pathelement location="${pdf-teacher.main.output.dir}"/>
    <path refid="library.gradle:_org.apache.pdfbox:pdfbox:2.0.13.classpath"/>
    <path refid="library.gradle:_com.google.guava:guava:28.0-jre.classpath"/>
    <path refid="library.gradle:_org.apache.pdfbox:fontbox:2.0.13.classpath"/>
